{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: dev (server)",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Next.js: chrome",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ],
  "compounds": [
    {
      "name": "Next.js: full",
      "configurations": ["Next.js: dev (server)", "Next.js: chrome"]
    }
  ]
}
